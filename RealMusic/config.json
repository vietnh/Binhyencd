  "CacheProfileSettings": {
    "CacheProfiles": {
      "Error": {
        "Duration": 86400,
        "Location": "Any"
      },
      "RobotsText": {
        "Duration": 86400,
        "Location": "Any"
      },
      "SitemapNodes": {
        "Duration": 86400
      }
    }
  }
}
