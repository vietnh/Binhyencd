{
  "AppSettings": {
    "SiteShortTitle": "Real Music",
    "SiteTitle": "Real Music"
  },
  "Logging": {
    "IncludeScopes": true,
    "LogLevel": {
      "Default": "Verbose",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "SitemapSettings": {
    "SitemapPingLocations": [
      "https://www.google.com/ping?sitemap=",
      "http://www.bing.com/ping?sitemap="
